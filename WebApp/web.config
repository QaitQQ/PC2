<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <system.webServer>
    <handlers>
      <add name="PythonHandler" path="*" verb="*" modules="httpPlatformHandler" resourceType="Unspecified"/>
	  
	      <add name="PythonHandler" path="*" verb="*" modules="FastCgiModule"
        scriptProcessor="D:\Python\python.exe|D:\Python\Scripts\wfastcgi"
        resourceType="Unspecified" requireAccess="Script"/>
    </handlers>
    <httpPlatform processPath="D:\Python\python.exe"
                  arguments="X:\Apps\Django\Django\manage.py runserver"
                  stdoutLogEnabled="true"
                  stdoutLogFile="X:\Apps\Django\Django\python.log"
                  startupTimeLimit="60"
                  processesPerApplication="16">
      <environmentVariables>
        <environmentVariable name="SERVER_PORT" value="%HTTP_PLATFORM_PORT%" />
      </environmentVariables>
    </httpPlatform>
  </system.webServer>
  <system.web>
    <compilation debug="true" />
  </system.web>
  <appSettings>
  <add key="PYTHONPATH" value="X:\Apps\Django\Django\"/>
  <!-- The handler here is specific to Bottle; see the next section. -->
<add key="WSGI_HANDLER" value="django.core.wsgi.get_wsgi_application()"/>
<add key="DJANGO_SETTINGS_MODULE" value="django_iis_example.settings" />
  <add key="WSGI_LOG" value="X:\Apps\Django\Django\wfastcgi.log"/>
</appSettings>
</configuration>