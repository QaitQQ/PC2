<UserControl x:Class="WinFormsClientLib.Forms.WPF.Controls.ItemControls.Market.MainMarketPlaceControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WinFormsClientLib.Forms.WPF.Controls.ItemControls.Market"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <TabControl TabStripPlacement="Left" SelectionChanged="TabControl_SelectionChanged">
            <TabItem Header="Заказы" x:Name="Orders">
                <TabItem.LayoutTransform>
                    <RotateTransform Angle="-90"></RotateTransform>
                </TabItem.LayoutTransform>
            </TabItem>
            <TabItem Header="Позиции" x:Name="Items">
                <TabItem.LayoutTransform>
                    <RotateTransform Angle="-90"></RotateTransform>
                </TabItem.LayoutTransform>
            </TabItem>
            <TabItem Header="Настройки" x:Name="Options">
                <TabItem.LayoutTransform>
                    <RotateTransform Angle="-90"></RotateTransform>
                </TabItem.LayoutTransform>
            </TabItem>
        </TabControl>


    </Grid>
</UserControl>
